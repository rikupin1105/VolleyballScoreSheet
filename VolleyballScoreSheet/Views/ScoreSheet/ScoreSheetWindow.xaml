<Window
    x:Class="VolleyballScoreSheet.Views.ScoreSheet.ScoreSheetWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:VolleyballScoreSheet.Views.ScoreSheet"
    xmlns:prism="http://prismlibrary.com/"
    prism:ViewModelLocator.AutoWireViewModel="True"
    SizeToContent="WidthAndHeight">
    <Grid>
        <StackPanel>
            <local:ScoreSheet />
            <Button Command="{Binding PrintCommand}" Content="印刷" />
        </StackPanel>
    </Grid>
</Window>
